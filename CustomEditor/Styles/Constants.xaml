<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:system="clr-namespace:System;assembly=mscorlib">
	<FontFamily x:Key="RegularFontFamily">./#Open Sans</FontFamily>

	<system:Double x:Key="RegularFontSize">13</system:Double>
	<system:Double x:Key="RegularHeight">30</system:Double>
	<system:Double x:Key="DisabledOpacity">0.5</system:Double>

	<CornerRadius x:Key="CornerRadius">2</CornerRadius>
</ResourceDictionary>